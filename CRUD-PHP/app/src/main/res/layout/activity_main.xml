<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".MainActivity">

	<ImageView
			android:layout_width="150dp"
			android:layout_height="150dp"
			android:src="@drawable/ic_register"
			tools:ignore="ContentDescription" android:layout_alignParentBottom="true"
			android:layout_alignParentEnd="true" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.498"
			app:layout_constraintVertical_bias="0.055" android:id="@+id/imageView2"/>

	<TextView
			android:layout_width="371dp"
			android:layout_height="27dp"
			android:text="Introduzca el nombre o apellido"
			android:textColor="#3e3e3e"
			android:textSize="16dp"
			tools:ignore="HardcodedText,MissingConstraints,SpUsage" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintVertical_bias="0.303" android:id="@+id/textView2"/>

	<EditText
			android:layout_width="370dp"
			android:layout_height="40dp"
			android:hint="Nombre del Contacto"
			tools:ignore="Autofill,HardcodedText,LabelFor,MissingConstraints,TextFields"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintHorizontal_bias="0.487" app:layout_constraintVertical_bias="0.367"/>

	<Button
			android:layout_width="150dp"
			android:layout_height="wrap_content"
			tools:ignore="MissingConstraints"
			android:textColor="#fff"
			android:text="Buscar"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.122"
			app:layout_constraintVertical_bias="0.449"/>
	<Button
			android:layout_width="150dp"
			android:layout_height="wrap_content"
			tools:ignore="MissingConstraints"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.827"
			app:layout_constraintVertical_bias="0.449" android:id="@+id/button"/>
</androidx.constraintlayout.widget.ConstraintLayout>